<div class="container is-flex is-flex-wrap-nowrap is-justify-content-space-around">
    <div class="column"></div>
    <div class="column is-8 tile is-ancestor">
        <div class="tile is-4 is-vertical is-parent">
            <div class="tile box is-child">
                <img class="is-fullwidth" src='images/red_block.png'>
            </div>
            <div class="tile box is-child">
                {{#if Data.west}}
                <img class="is-fullwidth" src='images/green_block.png'>
                {{else}}
                <img class="is-fullwidth" src='images/red_block.png'>
                {{/if}}
            </div>
            <div class="tile box is-child">
                <img class="is-fullwidth" src='images/red_block.png'>
            </div>
        </div>
        <div class="tile is-4 is-vertical is-parent">
            <div class="tile box is-child">
                {{#if Data.north}}
                <img class="is-fullwidth" src='images/green_block.png'>
                {{else}}
                <img class="is-fullwidth" src='images/red_block.png'>
                {{/if}}
            </div>
            <div class="tile box is-child">
                <img src='images/red_block.png'>
            </div>
            <div class="tile box is-child">
                {{#if Data.south}}
                <img class="is-fullwidth" src='images/green_block.png'>
                {{else}}
                <img class="is-fullwidth" src='images/red_block.png'>
                {{/if}}
            </div>
        </div>
        <div class="tile is-4 is-vertical is-parent">
            <div class="tile box is-child">
                <img src='images/red_block.png'>
            </div>
            <div class="tile box is-child">
                {{#if Data.east}}
                <img class="is-fullwidth" src='images/green_block.png'>
                {{else}}
                <img class="is-fullwidth" src='images/red_block.png'>
                {{/if}}
            </div>
            <div class="tile box is-child">
                <img src='images/red_block.png'>
            </div>
        </div>
    </div>
    <div class="column"></div>
</div>